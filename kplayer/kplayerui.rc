  <Separator lineSeparator="true"/>
  <Action name="playlist_lock"/>
  <Action name="playlist_selection"/>
  <Separator lineSeparator="true"/>
  <Action name="playlist_move_up"/>
  <Action name="playlist_move_down"/>
  <Separator lineSeparator="true"/>
