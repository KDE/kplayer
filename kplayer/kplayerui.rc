      <Separator lineSeparator="true"/>
      <Action name="subtitles_delay_decrease"/>
      <Action name="subtitles_delay_increase"/>
    </Menu>
  </Menu>
  <Menu name="playlist" noMerge="1"><text>P&amp;laylist</text>
    <Action name="playlist_show"/>
    <Action name="playlist_editor"/>
    <Action name="playlist_toolbar"/>
    <Separator lineSeparator="true"/>
    <Action name="playlist_next"/>
    <Action name="playlist_previous"/>
    <Separator lineSeparator="true"/>
    <Action name="playlist_loop"/>
    <Action name="playlist_shuffle"/>
    <Separator lineSeparator="true"/>
